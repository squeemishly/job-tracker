<table>
  <%= form_for [@company, @job] do |f| %>
    <tr><td><%= f.label :title %></td>
    <td><%= f.text_field :title %></td></tr>

    <tr><td><%= f.label :description %></td>
    <td><%= f.text_field :description %></td></tr>

    <tr><td><%= f.label :level_of_interest %></td>
    <td><%= f.number_field :level_of_interest %></td></tr>

    <tr><td><%= f.label :city %></td>
    <td><%= f.text_field :city %></td></tr>

    <tr><td><%= f.label :category_id %></td>
    <td><%= f.collection_select :category_id, Category.all, :id, :title, prompt: 'Select Category' %></td></tr>

    <tr><%= f.submit %><tr>
  <% end %>
</table>
